[
    {
        "title":"《春晓》",
        "paragraphs":[
            "春眠不觉晓",
            "处处闻啼鸟",
            "夜来风雨声",
            "花落知多少"
        ]
    },
    {
        "title":"《村居》",
        "paragraphs":[
            "草长莺飞二月天",
            "拂堤杨柳醉春烟",
            "儿童放学归来早",
            "忙趁东风放纸鸢"
        ]
    },
    {
        "title":"《静夜思》",
        "paragraphs":[
            "床前明月光",
            "疑似地上霜",
            "举头望明月",
            "低头思故乡"
        ]
    },
    {
        "title":"《悯农》",
        "paragraphs":[
            "锄禾日当午",
            "汗滴禾下土",
            "谁知盘中餐",
            "粒粒皆辛苦"
        ]
    },
    {
        "title":"《赠汪伦》",
        "paragraphs":[
            "李白乘舟将欲行",
            "忽闻岸上踏歌声",
            "桃花潭水深千尺",
            "不及汪伦送我情"
        ]
    },
    {
        "title":"《寻隐者不遇》",
        "paragraphs":[
            "松下问童子",
            "言师采药去",
            "只在此山中",
            "云深不知处"
        ]
    }
    
]