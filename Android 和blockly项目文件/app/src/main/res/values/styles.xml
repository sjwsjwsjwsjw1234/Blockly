<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!-- Base application theme. -->
    <style name="AppTheme.NoActionBar">
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="Splash" parent="AppTheme.NoActionBar">
        <item name="android:windowBackground">@drawable/splash_interface</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/white</item>
        <item name="colorControlActivated">@color/buttonYellow</item>
    </style>


    <style name="AppTheme.NoActionBarAndTitle">
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <!--Material样式-->
    <style name="MaterialStyle" parent="Theme.MaterialComponents.Light.NoActionBar"/>
    <style name="lable_style">
        <item name="android:background">#33ffffff</item>
        <item name="android:textColor">@android:color/holo_blue_light</item>
    </style>
</resources>